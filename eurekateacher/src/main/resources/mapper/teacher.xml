<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.eurekateacher.repository.TeacherMapper">

    <resultMap id="teacherResult" type="Teacher">
      <id property="id" column="id"/>
      <result property="name" column="name"/>
      <result property="age" column="age"/>
      <result property="born" column="born"/>

    </resultMap>

    <select id="select"  parameterType="long"  resultType="Teacher">
        select * from teacher where id = #{id}
    </select>

    <insert id="save" parameterType="Teacher">
        insert into teacher values(#{id},#{name},#{age},#{born})


    </insert>

</mapper>